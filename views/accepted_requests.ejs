<%- include('./partials/header') %> 

<div class="container counsellor-panel-div">
    <table class="table table-dark table-sm">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Phone no.</th>
                <th scope="col">Need help for?</th>
                <th scope="col">Role</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            <% var i=0 %>
            <% acc_req.forEach(function(req){ %>
            <tr>
                <% i+=1 %>
                <th scope="row"><%= i %></th>
                <td><%= req.name %></td>
                <td><%= req.email %></td>
                <td><%= req.phone %></td>
                <td><%= req.needHelpIn %></td>
                <td><%= req.role %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</div>

<%- include('./partials/footer') %>  